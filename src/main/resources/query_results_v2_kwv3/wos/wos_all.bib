Type = {Proceedings Paper},
Language = {English},
Affiliation = {Luo, ZP (Corresponding Author), Univ S Florida, Tampa, FL 33620 USA.
   Luo, Zhengping; Hou, Tao; Lu, Zhuo, Univ S Florida, Tampa, FL 33620 USA.
