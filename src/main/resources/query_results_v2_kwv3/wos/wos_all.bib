Author = {Yu Jiaxi and Yang Lin and Liu Ruiye and Guo Zhizhong},
Book-Group-Author = {IEEE},
Title = {Research on time process-oriented power system static security analysis},
Booktitle = {2008 THIRD INTERNATIONAL CONFERENCE ON ELECTRIC UTILITY DEREGULATION AND
   RESTRUCTURING AND POWER TECHNOLOGIES, VOLS 1-6},
Year = {2008},
