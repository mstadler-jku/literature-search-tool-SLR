@article{ WOS:000292055400016,
Author = {Wang, Li and Song, Mei and Wang, Jingyao and Zhang, Yong},
Title = {A Novel Energy-efficient Authentication Data Distribution Scheme for
   Heterogeneous Interworking Networks},
Journal = {INFORMATION-AN INTERNATIONAL INTERDISCIPLINARY JOURNAL},
Year = {2011},
Volume = {14},
Number = {4},
Pages = {1303-1313},
Month = {APR},
Abstract = {This paper firstly puts forward an energy-efficient authentication data
   distribution scheme, which is composed of two parts, one is for relative
   statistic occasions when multi-mode UE (User Equipment) moves only in
   its own home domain, the other one is for dynamic cases especially when
   UE transfers frequently between different domains. Meanwhile, an
   improved authentication protocol based on authentication vectors (AV) is
   proposed, and the relative security analysis is given as well. Similar
   as EAP-AKA, we assume that the first registered 3GPP AAA server in home
   domain (i.e. HPLMN, Home Public Land Mobile Network) takes charge of the
   responsibility of authentication for 3GPP/WiMAX UE by using AVs obtained
   from HSS (Home Subscriber Server). Using of this scheme, it is
   unnecessary to retrieve new AVs from HSS, if there are still unused AVs
   for the previous PLMN when the multi-mode UE leaves and reenters the
   same PLMN within its available period of critical period interval (CPI).
   Theoretical analysis show that a long CPI results in fewer accesses to
   the HSS, and only through adjusting the CPI period dynamically in
   accordance with the system parameters, the signaling cost can be reduced
   further, so as to achieve the energy-efficient purpose.},
