@inproceedings{ WOS:000221854800061,
Author = {Aung, KMM and Park, JS},
Editor = {Lagana, A and Gavrilova, ML and Kumar, V and Mun, Y and Tan, CJK and Gervasi, O},
Title = {Software rejuvenation approach to security engineering},
Booktitle = {COMPUTATIONAL SCIENCE AND ITS APPLICATIONS - ICCSA 2004, PT 4},
