@article{ WOS:000327415000007,
Author = {Pisica, Ioana and Taylor, Gareth and Lipan, Laurentiu},
Title = {Feature selection filter for classification of power system operating
   states},
Journal = {COMPUTERS \& MATHEMATICS WITH APPLICATIONS},
