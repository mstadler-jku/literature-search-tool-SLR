@inproceedings{ WOS:000426901200067,
Author = {Thomas, Benny and Thrivani, J. and Venugopal, K. R.},
Book-Group-Author = {IEEE},
Title = {An Efficient Cloud based Architecture for Integrating Content Management
   Systems},
Booktitle = {2017 INTERNATIONAL CONFERENCE ON INNOVATIVE MECHANISMS FOR INDUSTRY
