Book-Group-Author = {IEEE},
Title = {Tracklet and Signature Representation for Multi-Shot Person
   Re-Identification},
Booktitle = {2018 15TH INTERNATIONAL MULTI-CONFERENCE ON SYSTEMS, SIGNALS AND DEVICES
   (SSD)},
Series = {International Multi-Conference on Systems Signals and Devices},
Year = {2018},
Pages = {214-219},
