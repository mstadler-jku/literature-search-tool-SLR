Title = {Approximating Non-interference and Erasure in Rewriting Logic},
Booktitle = {12TH INTERNATIONAL SYMPOSIUM ON SYMBOLIC AND NUMERIC ALGORITHMS FOR
   SCIENTIFIC COMPUTING (SYNASC 2010)},
