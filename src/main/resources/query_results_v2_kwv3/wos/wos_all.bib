@inproceedings{ WOS:000365096100003,
Author = {Meng, Tianhui and Wolter, Katinka and Wang, Qiushi},
Editor = {Beltran, M and Knottenbelt, W and Bradley, J},
Title = {Security and Performance Tradeoff Analysis of Mobile Offloading Systems
