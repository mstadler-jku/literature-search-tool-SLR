@article{ WOS:000425606900022,
Author = {Yeh, Kuo-Hui and Su, Chunhua and Chiu, Wayne and Zhou, Lu},
Title = {I Walk, Therefore I Am: Continuous User Authentication with Plantar
   Biometrics},
