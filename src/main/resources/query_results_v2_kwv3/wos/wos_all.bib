@article{ WOS:000282837700039,
Author = {Yuan, Li and Xu, Jian},
Title = {A method for analyzing the oil pipelines system to reduce the damage to
   the ecological environment},
Journal = {DISASTER ADVANCES},
Year = {2010},
Volume = {3},
Number = {4},
Pages = {206-209},
Month = {OCT},
Abstract = {In recent years, too many oil pipelines system explosion occurred at
   home and abroad, which has brought great pollution to the ecological
   environment. In this paper, a consecutive-k-out-of-n: F system is used
   to model the security problem of the oil transportation system. In this
   model, we assume the oil pipeline system has n identical pump stations
   and R repairmen taking charge of the pump stations. The working times of
   the pump stations and the repair times of the failed pump stations are
   assumed to be independent and identically exponential distributions. All
   vacations of the repairmen are independent and identically distributed
   as an exponential. Under these assumptions, by the pairs (i, j), the
   states of the system are listed. Therefore, the state transition rate
   matrix is obtained. The reliability indexes of the oil pipeline system
   can be obtained, too. Finally, the Run ge-Kutta method is used to a
   special case, and the result is that the structure of the system itself
   affects the security significantly comparing with the external repair
   service.},
