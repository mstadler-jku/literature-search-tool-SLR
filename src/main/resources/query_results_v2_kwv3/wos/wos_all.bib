@article{ WOS:000657561500001,
Author = {Takayasu, Atsushi},
Title = {Tag-based ABE in prime-order groups via pair encoding},
Journal = {DESIGNS CODES AND CRYPTOGRAPHY},
Year = {2021},
Volume = {89},
Number = {8},
Pages = {1927-1963},
Month = {AUG},
Abstract = {Predicate/pair encodings are simple frameworks for designing
   attribute-based encryption (ABE) for complex predicates, with pair
   encodings being able to handle more complex predicates. Thus far,
   several generic constructions of prime-order ABE schemes have been
   proposed with these encodings. Chen, Gay, and Wee (CGW) (Eurocrypt' 15)
   and Chen and Gong (CG) (Asiacrypt' 17) proposed generic constructions
   with predicate encodings with a trade-off in efficiency. In particular,
   the former construction (CGW ABE) has the shorter secret keys, whereas
   the latter construction (CG ABE) has the shorter master public keys and
   ciphertexts. Moreover, CG ABE requires three pairing operations during
   decryption, while CGW ABE requires four. Agrawal and Chase (AC) (TCC'
   16) proposed a generic construction with pair encodings that is an
   extension of CGW ABE and can handle more complex predicates.
   Specifically, if pair encoding schemes satisfy perfect security (resp.
   relaxed perfect security), then AC ABE satisfies full security (resp.
   semi-adaptive security) from the standard k-linear assumption. However,
   there is no extension of CG ABE with pair encodings. In this paper, we
   construct this extension. As with the trade-off between CGW ABE and CG
   ABE, our proposed ABE has shorter master public keys and ciphertexts and
   larger secret keys, requires less pairing operations during decryption
   than AC ABE. Furthermore, as with AC ABE, our proposed ABE satisfies
   full security (resp. semi-adaptive security) if pair encoding schemes
   satisfy perfect security (resp. relaxed perfect security) from the
   standard k-linear assumption. As an application, we propose a
   ciphertext-policy ABE scheme for non-monotone span programs with compact
   ciphertexts satisfying semi-adaptive security.},
