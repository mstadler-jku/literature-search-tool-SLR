@inproceedings{ WOS:000223908800002,
Author = {Takahashi, K and Mimura, M and Isobe, Y and Seto, Y},
Editor = {Jain, AK and Ratha, NK},
Title = {A secure and user-friendly multi-modal biometric system},
Booktitle = {BIOMETRIC TECHNOLOGY FOR HUMAN IDENTIFICATION},
Series = {PROCEEDINGS OF THE SOCIETY OF PHOTO-OPTICAL INSTRUMENTATION ENGINEERS
   (SPIE)},
Year = {2004},
Volume = {5404},
Pages = {12-19},
Note = {Conference on Biometric Technology for Human Identification, Orlando,
   FL, APR 12-13, 2004},
Organization = {SPIE},
Abstract = {Biometric verification has attracted attention recently because it is
   more secure than knowledge- or token-based verification techniques.
   Multi-modal biometric verification can provide even greater accuracy by
   combining several forms of biometrics. However, there are problems with
   the availability, usability and acceptability of the technique. In this
   paper, we take a new approach in proposing a multi-modal biometric
   system that enables users to select which biometrics they prefer to be
   matched at the time of verification. This system also reduces the number
   of inputs required by adopting a sequential test based on statistical
