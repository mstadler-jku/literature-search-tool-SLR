@article{ WOS:000686114200001,
Author = {Wei, Peng and Wang, Shilian and Luo, Junshan and Liu, Yan and Hu, Li},
Title = {Optimal frequency-hopping anti-jamming strategy based on multi-step
   prediction Markov decision process},
