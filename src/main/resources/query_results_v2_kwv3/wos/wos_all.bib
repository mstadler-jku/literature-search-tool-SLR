Author = {Kancharla, Abhilash and Seol, Jongho and Park, Nicole and Park, Indy and
   Park, Nohpill},
Book-Group-Author = {IEEE},
Title = {Dependable Industrial Crypto Computing},
Booktitle = {2019 IEEE 28TH INTERNATIONAL SYMPOSIUM ON INDUSTRIAL ELECTRONICS (ISIE)},
