@inproceedings{ WOS:000278095700014,
Author = {Bellare, Mihir and Brakerski, Zvika and Naor, Moni and Ristenpart,
   Thomas and Segev, Gil and Shacham, Hovav and Yilek, Scott},
Editor = {Matsui, M},
Title = {Hedged Public-Key Encryption: How to Protect against Bad Randomness},
Booktitle = {ADVANCES IN CRYPTOLOGY - ASIACRYPT 2009},
