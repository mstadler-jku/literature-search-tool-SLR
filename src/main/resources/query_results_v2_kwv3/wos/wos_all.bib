Type = {Article},
Language = {English},
Affiliation = {Qian, XS (Corresponding Author), Macau Univ Sci \& Technol, Inst Syst Engn, Macau 999078, Peoples R China.
   Qian, XS (Corresponding Author), Macau Univ Sci \& Technol, Collaborat Lab Intelligent Sci \& Syst, Macau 999078, Peoples R China.
