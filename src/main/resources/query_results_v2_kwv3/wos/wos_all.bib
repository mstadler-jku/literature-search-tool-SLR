@article{ WOS:000731141800001,
Author = {Ma, Longyu and Sham, Chiu-Wing and Lo, Chun Yan and Zhong, Xinchao},
Title = {An Effective Multi-Mode Iris Authentication System on a
   Microprocessor-FPGA Heterogeneous Platform With QC-LDPC Codes},
Journal = {IEEE ACCESS},
Year = {2021},
Volume = {9},
Pages = {163665-163674},
Abstract = {With the emergence and popularity of iris biometrics, there are
   increasing concerns regarding the feasibility of iris authentication
   systems and their corresponding variability reduction methods. The
   former issues are typically addressed by optimizing key factors, such as
   iris size, image quality and acquisition wavelength. As for the latter,
   introducing error correction codes to reduce intra-user variability in
   the enrolled identifiers becomes novelly promising. This paper proposes
   a conventional iris authentication system and a hardware-friendly
   QC-LDPC error correction code scheme on a microprocessor-FPGA platform.
   Different QC-LDPC codes in IEEE 802.16e were analyzed and selected.
   Suitable codes were applied, followed by the evaluation experiments. The
   proposed design achieves a competitive result with up to 0.20\% EER and
   0.50\% ZeroFAR on the CASIA-IrisV4-Syn database. Cryptographic keys with
   lengths of up to 288 bits can also be generated and recovered. Such a
   device can be potentially used for applications such as an access
   control system in high-security areas, identity verification at the
   borders, biometric cryptography and related authentication scenarios.},
