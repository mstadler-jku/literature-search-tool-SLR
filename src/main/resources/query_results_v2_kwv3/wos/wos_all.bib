@article{ WOS:000795479800001,
Author = {Zhai, Chao and Nguyen, Hung D. and Zong, Xiaofeng},
Title = {Dynamic Security Assessment of Small-Signal Stability for Power Grids
   Using Windowed Online Gaussian Process},
