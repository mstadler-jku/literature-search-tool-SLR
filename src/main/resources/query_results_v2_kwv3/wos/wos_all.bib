@inproceedings{ WOS:000080449600180,
Author = {El-Shibini, MA and Saied, EM},
Editor = {McBride, AE},
Title = {Steady state security assessment through two stages neural network
   approach},
Booktitle = {PROCEEDINGS OF THE AMERICAN POWER CONFERENCE, VOL 61 - I AND II},
Series = {PROCEEDINGS OF THE AMERICAN POWER CONFERENCE},
Year = {1999},
Volume = {61},
Number = {1\&2},
Pages = {935-939},
Note = {American Power Conference, at the 61st Annual Meeting of the
   Illinois-Institute-of-Technology on Innovation for Economy and
   Reliability, CHICAGO, IL, 1999},
Organization = {Illinois Inst Technol},
Abstract = {A technique which depends on the application of the neural network for
   the steady state security assessment is suggested in this paper. The
   paper gives high accuracy security classification without the need for a
   very big size training set. The problem has been solved in two stages.
   In the first stage an approximate classifier is to be found
   corresponding to a reasonable number of operating states. The neural
   network used in this stage has no hidden layers,as only approximate
   classifier is needed, which reduces the training procedure effort. In
   the second stage a training set is then to be generated in the space
   around this approximate classifier using an appropriate step by step
   mathematical technique. A multilayer neural network is then to be
   trained by this new training set to get a high accuracy classifier.
   A corrective action is also suggested. When insecure case is detected,in
   the real life operation, corrective action must take place to take the
   system from the insecure to the secure state. This call be done by using
   the approximate classifier expression and the power balance equation at
   first to move the insecure operating point to the secure region. The new
   operating point is then to be checked using the high accuracy
   classifier, if found insecure it can be easily moved to a new point and
   then rechecked until the point attains the security state. The suggested
   technique has been tested on a sample power system and gave very
   satisfactory results.},
