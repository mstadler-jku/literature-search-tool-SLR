@inproceedings{ WOS:000468051000040,
Author = {Yang, Xuetao and Song, Dunwen and Tang, Xiaojun and Song, Xinli and Xu,
   Pengfei and Li, Zheng},
Book-Group-Author = {IEEE},
Title = {Practicable Cascading Failure Process Prediction Analysis Scheme and
   System Development Based on Multiple Information Sources},
Booktitle = {2018 INTERNATIONAL CONFERENCE ON POWER SYSTEM TECHNOLOGY (POWERCON)},
Series = {International Conference on Power System Technology},
Year = {2018},
Pages = {260-265},
Note = {International Conference on Power System Technology (POWERCON),
   Guangzhou, PEOPLES R CHINA, NOV 06-08, 2018},
Organization = {Chinese Soc Elect Engn; IEEE Power \& Energy Soc; China So Power Grid Co
   Ltd; S China Univ Technol; Guangdong Power Grid Co Ltd},
Abstract = {Power grids in China are in the stage of rapid development of UHV AC and
   DC. Cascading failures with timing and propagation characteristics
   become the main threat to power grid security. Based on the EMS and WAMS
   information and according to the principle of practicality, a
   practicable scheme for cascading failure prediction and analysis is
   proposed. And the related analysis system has been developed. In line
   with the timing development characteristics of cascading failures, the
   overall system layered architecture and key technologies are proposed.
   The access relationship of the main sub-function modules of the system
