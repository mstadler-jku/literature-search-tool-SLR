@inproceedings{ WOS:000291324200006,
Author = {Alba-Castro, Mauricio and Alpuente, Maria and Escobar, Santiago},
Editor = {DeBoer, FS and Bonsangue, MM and Hallerstede, S and Leuschel, M},
Title = {Abstract Certification of Global Non-interference in Rewriting Logic},
