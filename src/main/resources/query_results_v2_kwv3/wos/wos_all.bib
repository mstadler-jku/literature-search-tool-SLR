@article{ WOS:000208787700017,
Author = {Qiu, Meikang and Gao, Wenzhong and Chen, Min and Niu, Jian-Wei and
   Zhang, Lei},
Title = {Energy Efficient Security Algorithm for Power Grid Wide Area Monitoring
   System},
Journal = {IEEE TRANSACTIONS ON SMART GRID},
