@article{ WOS:000508186400171,
Author = {Zhou, Suyang and Hu, Zijian and Zhong, Zhi and He, Di and Jiang, Meng},
Title = {An Integrated Energy System Operating Scenarios Generator Based on
   Generative Adversarial Network},
