@inproceedings{ WOS:000426631900130,
Author = {Wang, Pangwei and Jiang, Chuan and Deng, Xiaohui and Wang, Li and Deng,
   Hui and He, Zhonghe},
Editor = {Sun, M and Gao, H},
Title = {A Multi-mode Cooperative Adaptive Cruise Switching Control Model for
   Connected Vehicles Considering Abnormal Communication},
Booktitle = {2017 6TH DATA DRIVEN CONTROL AND LEARNING SYSTEMS (DDCLS)},
Year = {2017},
Pages = {739-744},
Note = {6th IEEE Data Driven Control and Learning Systems Conference (DDCLS),
   Chongqing, PEOPLES R CHINA, MAY 26-27, 2017},
Organization = {IEEE; Chinese Assoc Automat, Tech Comm Data Driven Control Learning \&
   Optimizat; Chongqing Jiaotong Univ; IEEE Beijing Sect; IEEE Ind Elect
   Soc; Beijing Jiaotong Univ},
Abstract = {With the development of vehicle-to-vehicle
   (V2V)/vehicle-to-infrastructure (V2I) communication systems, the spacing
   between two vehicles is shortened effectively and the traffic efficiency
   is improved. Meanwhile, abnormal wireless communication caused by
   time-delay, packet-dropout and failure brings the new traffic security
   problem. In this paper, the connected vehicle platoons is taken as the
   research object. Firstly, a traditional cooperative adaptive cruise
   control (CACC) system is established based on distributed controller;
   secondly, according to the influence of abnormal wireless communication,
   a multi-mode switching control model is presented to improve the CACC
   controller; thirdly, according to the conditions of string stability,
   the ranges of controller parameters and constant time headway (CTH) are
   obtained; finally, the feasibility of the controller is simulated based
   on Matlab/Simulink. The test results show that the multi-mode CACC
   controller can not only avoid the influence of abnormal communication on
   intelligent connected vehicle platoon, but also ensure the string
   stability under the smaller driving spacing in the meantime.},
Publisher = {IEEE},
Address = {345 E 47TH ST, NEW YORK, NY 10017 USA},
