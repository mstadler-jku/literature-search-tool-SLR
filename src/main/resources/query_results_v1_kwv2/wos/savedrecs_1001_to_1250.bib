@article{ WOS:001018715500001,
Author = {Jia, Rui and Ni, Xiaoli and Shao, Xiaoyi and Zhang, Ting and Qi, Qunkun},
Title = {Development of a Security Education Model System for Cloud Computing and
   Data Security and Privacy Protection},
Journal = {WIRELESS PERSONAL COMMUNICATIONS},
Year = {2023},
Month = {2023 JUL 3},
Abstract = {The great efficiency and dependability of cloud computing enhance the
   efficiency of mining and analyzing large data in education. Big data in
   education have been more prevalent in the cloud environment, and the
   efficiency of data processing has been greatly enhanced, elevating the
   significance of big data in education and teaching practice. However,
   the promotion of education big data in the cloud environment has
   revealed an increasing number of issues. People pay increasing attention
   to the security of big data in education, i.e., whether big data in
   education can be safely maintained, retrieved, and utilized in the cloud
   environment. It focuses on the relationships between education big data
   producers, users, and cloud service providers. The primary topics of
   this article are, therefore, how to govern the connections between the
   three parties, the adoption of stringent technical means, and the
   establishment of data use guidelines and a regulatory structure.
   Consequently, this paper investigated the multimodal information fusion
   technology and the data security model of education in colleges and
   universities, as well as constructing a cognitive psychological model
   for education in colleges and universities based on multimodal
   information fusion and conducting experimental research on it. The
   research showed that under the cognitive psychology mode of education in
   colleges and universities based on multi-modal information fusion, the
   cognitive efficiency of education of students has increased.},
