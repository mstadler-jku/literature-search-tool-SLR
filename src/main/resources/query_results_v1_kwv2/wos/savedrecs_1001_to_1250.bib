@inproceedings{ WOS:000269934400039,
Author = {Abdalla, Michel and Chevalier, Celine and Pointcheval, David},
Editor = {Halevi, S},
Title = {Smooth Projective Hashing for Conditionally Extractable Commitments},
Booktitle = {ADVANCES IN CRYPTOLOGY - CRYPTO 2009},
Series = {Lecture Notes in Computer Science},
