@inproceedings{ WOS:000591516700002,
Author = {Agrawal, Shweta and Yamada, Shota},
Editor = {Canteaut, A and Ishai, Y},
Title = {Optimal Broadcast Encryption from Pairings and LWE},
Booktitle = {ADVANCES IN CRYPTOLOGY - EUROCRYPT 2020, PT I},
