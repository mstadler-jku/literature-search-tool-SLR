@inproceedings{ WOS:000874466500008,
Author = {Delerablee, Cecile and Gouriou, Lenaick and Pointcheval, David},
Editor = {Galdi, C and Jarecki, S},
Title = {Key-Policy ABE with Switchable Attributes},
