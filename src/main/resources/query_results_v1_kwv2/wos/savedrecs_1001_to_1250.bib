@inproceedings{ WOS:000251184300008,
Author = {Maurer, Ueli and Pietrzak, Krzysztof and Renner, Renato},
Editor = {Menezes, A},
Title = {Indistinguishability amplification},
Booktitle = {ADVANCES IN CRYPTOLOGY - CRYPTO 2007, PROCEEDINGS},
