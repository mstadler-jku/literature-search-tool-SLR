@inproceedings{ WOS:000241753100012,
Author = {Lee, Dae-Jong and Kwon, Man-Jun and Chun, Myung-Geun},
Editor = {King, I and Wang, J and Chan, L and Wang, DL},
Title = {Secure personnel authentication based on multi-modal biometrics under
   ubiquitous environments},
Booktitle = {NEURAL INFORMATION PROCESSING, PT 2, PROCEEDINGS},
