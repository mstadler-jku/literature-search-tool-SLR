@article{ WOS:000502767100019,
Author = {Dou, Chunxia and Teng, Shengqi and Zhang, Tengfei and Zhang, Bo and Ma,
   Kai},
Title = {Layered management and hybrid control strategy based on hybrid automata
   and random forest for microgrid},
Journal = {IET RENEWABLE POWER GENERATION},
Year = {2019},
Volume = {13},
