   and Guo, Cui and Yu, Liangmin and Murto, Petri and Xu, Xiaofeng},
Title = {Marine biomass-derived, hygroscopic and temperature-responsive hydrogel
   beads for atmospheric water harvesting and solar-powered irrigation},
Journal = {JOURNAL OF MATERIALS CHEMISTRY A},
Year = {2022},
Volume = {10},
