@inproceedings{ WOS:000897681900007,
Author = {Barthoulot, Anais and Blazy, Olivier and Canard, Sebastien},
Editor = {Beresford, AR and Patra, A and Bellini, E},
Title = {(Augmented) Broadcast Encryption from Identity Based Encryption with
   Wildcard},
