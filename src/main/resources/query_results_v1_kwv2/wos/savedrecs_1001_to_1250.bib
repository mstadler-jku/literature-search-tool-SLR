@article{ WOS:000471721800009,
Author = {Su, Guan-Ming and Zhang, Chengcui and Wang, Haohong and Chen, Min and
   Kankanhalli, Mohan and Lienhart, Rainer},
Title = {Pushing the Boundary of Multimedia Big Data: An Overview of IEEE MIPR},
Journal = {IEEE MULTIMEDIA},
